<div class="row">
    <div class="col">
        <div id="mapPoiCommune" style="width:100%; height:400px"></div>
    </div>
</div>
<div class="row">
    <div class="col d-flex flex-row justify-content-around align-items-center align-content-center" style="height:44px;">
        <div class="form-check" style="width:auto;min-width:13px;">
            <input class="form-check-input" type="checkbox" id="checkboxBar">
            <label class="form-check-label" for="checkboxBar">bars</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="checkboxEcoles">
            <label class="form-check-label" for="checkboxEcoles">écoles</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="checkboxHopital">
            <label class="form-check-label" for="checkboxHopital">hopitaux</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="checkboxRestaurent">
            <label class="form-check-label" for="checkboxRestaurent">restaurents</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="checkboxStation">
            <label class="form-check-label" for="checkboxStation">bus</label>
        </div>
        <div class="form-check">
            <input class="form-check-input" type="checkbox" id="checkboxPark">
            <label class="form-check-label" for="checkboxPark">park</label>
        </div>
    </div>
</div>
<div class="row">
        <div class="col-lg-4">
            <h3 style="text-align:center">Hopitaux</h3>
            <ul id="hopital" class="listPOI list-group"></ul>
            <br />
            <h3 style="text-align:center">bars</h3>
            <ul id="bar" class="listPOI list-group"></ul>
        </div>
        <div class="col-lg-4">
            <h3 style="text-align:center">parcs</h3>
            <ul id="park" class="listPOI list-group"></ul>
            <br />
            <h3 style="text-align:center">resto</h3>
            <ul id="resto" class="listPOI list-group"></ul>
        </div>
        <div class="col-lg-4">
            <h3 style="text-align:center">écoles</h3>
            <ul id="ecole" class="listPOI list-group"></ul>
            <br />
            <h3 style="text-align:center">Station</h3>
            <ul id="station" class="listPOI list-group"></ul>
        </div>
</div>
<input id="latitude" value="@ViewBag.latitude" style="display:none" />
<input id="longitude" value="@ViewBag.longitude" style="display:none" />

<script src="~/Scripts/PoiCommuneScripts.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCHaJXOb3KVvODTc1FDLpY44cpoUctO0u0&libraries=places&callback=initMap" async defer></script>