<div id="mapPoiCommune" style="width:100%; height:400px"></div>
<br />
<div class="container">
    <div class="row">
        <div id="checkZonePOI" class="col-lg-12" style="text-align:center; position:relative">
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxBar" value="bars">
                <label class="form-check-label" for="checkboxBar">bars</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxEcoles" value="écoles">
                <label class="form-check-label" for="checkboxEcoles">écoles</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxHopital" value="hopitaux">
                <label class="form-check-label" for="checkboxHopital">hopitaux</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxRestaurent" value="restaurent">
                <label class="form-check-label" for="checkboxRestaurent">restaurent</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxStation" value="bus">
                <label class="form-check-label" for="checkboxStation">bus</label>
            </div>
            <div class="form-check form-check-inline">
                <input class="form-check-input" type="checkbox" id="checkboxPark" value="park">
                <label class="form-check-label" for="checkboxPark">park</label>
            </div>
        </div>


    </div>
    <br />
    <div class="row" style="font-size:small">
        <div  class="col-lg-4" >
            <div id="HospitalListPOI" >
                <div class="listPOI">
                <h3 style="text-align:center" data-toggle="collapse" href="#CollapseHospital" role="button" aria-expanded="false" aria-controls="CollapseHospital">Hopitaux   <span id="nbHospitals" class="badge badge-primary badge-pill"></span></h3>
                </div>

                <div class="collapse cusorArrow" id="CollapseHospital">
                    <ul id="hopital" class="list-group"></ul>
                </div>
            </div>

            <br />
            <div id="barListPOI">
                <div class="listPOI">
                    <h3 style="text-align:center" data-toggle="collapse" href="#CollapseBar" role="button" aria-expanded="false" aria-controls="CollapseBar">bars   <span id="nbBars" class="badge badge-primary badge-pill"></span></h3>
                </div>
                    <div class="collapse cusorArrow" id="CollapseBar">
                        <ul id="bar" class="list-group"></ul>
                    </div>
                </div>
        </div>
        <div class="col-lg-4">
            <div id="ParkListPOI">
                <div class="listPOI">
                    <h3 style="text-align:center" data-toggle="collapse" href="#CollapsePark" role="button" aria-expanded="false" aria-controls="CollapsePark">parcs   <span id="nbParks" class="badge badge-primary badge-pill"></span></h3>
                </div>
                    <div class="collapse cusorArrow" id="CollapsePark">
                        <ul id="park" class="list-group"></ul>
                    </div>
                </div>
            <br />
            <div id="RestaurantListPOI">
                <div class="listPOI">
                    <h3 style="text-align:center" data-toggle="collapse" href="#CollapseResto" role="button" aria-expanded="false" aria-controls="CollapseResto">resto   <span id="nbRestos" class="badge badge-primary badge-pill"></span></h3>
                </div>
                    <div class="collapse cusorArrow" id="CollapseResto">
                        <ul id="resto" class="list-group"></ul>
                    </div>
                </div>
        </div>
        <div class="col-lg-4">
            <div id="SchoolListPOI">
                <div class="listPOI">
                    <h3 style="text-align:center" data-toggle="collapse" href="#CollapseSchool" role="button" aria-expanded="false" aria-controls="CollapseSchool">écoles   <span id="nbSchools" class="badge badge-primary badge-pill"></span></h3>
                </div>
                    <div class="collapse cusorArrow" id="CollapseSchool">
                        <ul id="ecole" class="list-group"></ul>
                    </div>
                </div>
            <br />
            <div id="StationListPOI">
                <div class="listPOI">
                    <h3 style="text-align:center" data-toggle="collapse" href="#CollapseStation" role="button" aria-expanded="false" aria-controls="CollapseStation">station   <span id="nbStations" class="badge badge-primary badge-pill"></span></h3>
                </div>
                    <div class="collapse cusorArrow" id="CollapseStation">
                        <ul id="station" class="list-group"></ul>
                    </div>
                </div>
        </div>

    </div>
</div>
<input id="latitude" value="@ViewBag.latitude" style="display:none" />
<input id="longitude" value="@ViewBag.longitude" style="display:none" />

<script src="~/Scripts/PoiAdresseScripts.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCHaJXOb3KVvODTc1FDLpY44cpoUctO0u0&libraries=places&callback=initMap" async defer></script>
